interface ContentType {
  id: Int!
  uuid: String!
  name: String!
  entity: String!
  type: String!
  url: String!
  created: String!
  changed: String!
  language: String
  author: Author
}

type Article implements ContentType {
  id: Int!
  uuid: String!
  name: String!
  entity: String!
  type: String!
  url: String!
  created: String!
  changed: String!
  language: String
  author: Author

  published: Boolean!
  channel: Channel
  tags: [Tag]
  seoTitle: String!
  content: [Paragraph]
}

type Tag implements ContentType {
  id: Int!
  uuid: String!
  name: String!
  entity: String!
  type: String!
  url: String!
  created: String!
  changed: String!
  language: String
  author: Author

  published: Boolean!
  content: [Paragraph]
}

type Channel implements ContentType {
  id: Int!
  uuid: String!
  name: String!
  entity: String!
  type: String!
  url: String!
  created: String!
  changed: String!
  language: String
  author: Author

  published: Boolean!
  content: [Paragraph]
}

type Author implements ContentType {
  id: Int!
  uuid: String!
  name: String!
  entity: String!
  type: String!
  url: String!
  created: String!
  changed: String!
  language: String
  author: Author

  mail: String
}
