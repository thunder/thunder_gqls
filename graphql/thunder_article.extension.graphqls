extend type Query {
  article(id: Int!): Article
}

extend type Article {
  id: Int!
  uuid: String!
  entity: String!
  type: String!
  title: String!
  url: String!
  created: String!
  changed: String!
  language: String!
  published: Boolean!
  channel: Channel
  tags: [Tag]
  seoTitle: String!
  content: [Paragraph]
}
